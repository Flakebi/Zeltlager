<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Zeltlager.GeneralPage"
             Title="Allgemeines" Style="{StaticResource BaseStyle}">
  <ContentPage.Content>
    <StackLayout VerticalOptions="FillAndExpand"
                 HorizontalOptions="FillAndExpand"
                 Orientation="Vertical"
                 Spacing="15">
      <Button x:Name="memberButton" Text="Teilnehmer" Clicked="OnMemberClicked"/>
      <Button x:Name="tentButton" Text="Zelte" Clicked="OnTentClicked"/>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>